<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.logintest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.logintest.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.logintest.ManageActivity"
            android:label="@string/title_activity_manage" >
        </activity>
        <activity
            android:name="com.example.logintest.AddOutaccount"
            android:label="@string/title_activity_add_outaccount" >
        </activity>
        <activity
            android:name="com.example.logintest.AddInaccount"
            android:label="@string/title_activity_add_inaccount" >
        </activity>
        <activity
            android:name="com.example.logintest.Outaccountinfo"
            android:label="@string/title_activity_outaccountinfo" >
        </activity>
        <activity
            android:name="com.example.logintest.InfoManage"
            android:label="@string/title_activity_info_manage" >
        </activity>
        <activity
            android:name="com.example.logintest.Inaccountinfo"
            android:label="@string/title_activity_inaccountinfo" >
        </activity>
        <activity
            android:name="com.example.logintest.Showinfo"
            android:label="@string/title_activity_showinfo" >
        </activity>
    </application>

</manifest>
